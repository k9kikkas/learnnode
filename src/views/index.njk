{% extends 'src/views/partials/layout.njk' %}
{% block content %}
    <h1>Minecraft</h1>
    <h3>Hello {{ name }}</h3>
    <h5>Fruits:</h5>
    <ul>
        {% for fruit in fruits %}
            <li>
                {{ fruit }}</li>
        {% endfor %}
    </ul>
    <ul>
        {% for i in range(1, 9) %}
            <li>
                <a href="/page{{ i }}.html">Page {{ i }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}